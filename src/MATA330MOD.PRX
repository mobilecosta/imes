		aVfim[nX] := &(Eval(bBloco,"TRT->TRB_VFIM",nX))
		aCM[nX]   := aVFIM[nX]/ABS(TRB_QFIM)
	Next nX
	Replace TRB_CM1 With aCM[01]
	If "2" $ cMV_MOEDACM
		Replace TRB_CM2 With aCM[02]
	EndIf
	If "3" $ cMV_MOEDACM
